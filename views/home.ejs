<%- include("partials/header"); -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach( function (element) { %>
    <h2><%= element.title %></h2>
    <p>
        <%= element.content.slice(0, 100) + " ..." %>
        <a href="/posts/<%= element.title %>">Read more</a> 
    </p>
<% }); %>
 

<%- include("partials/footer"); -%>
